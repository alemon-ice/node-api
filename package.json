{
  "name": "devlab-api",
  "version": "1.0.0",
  "description": "Typescript Node.js API",
  "main": "index.js",
  "author": "Rafael Dias <rafaeldiasdesouza.rd@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/index.js",
    "dev:server": "tsnd 'src/index.ts'",
    "test:func": "jest --projects ./test --runInBand",
    "test:unit": "jest",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "prettier:check": "prettier --check 'src/**/*.ts' 'test/**/*'",
    "prettier:fix": "prettier --write 'src/**/*.ts' 'test/**/*'"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/config": "^0.0.38",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/module-alias": "^2.0.0",
    "@types/mongoose": "^5.10.3",
    "@types/node": "^14.14.21",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "eslint": "^7.18.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "supertest": "^6.1.1",
    "ts-jest": "^26.4.4",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.5",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "config": "^3.3.3",
    "express": "^4.17.1",
    "module-alias": "^2.2.2",
    "mongoose": "^5.11.13"
  }
}
